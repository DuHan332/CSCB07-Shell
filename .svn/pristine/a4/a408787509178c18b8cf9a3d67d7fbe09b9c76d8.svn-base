package outputformatter;

/**
 * This class is implement from interface Output and would print nothing for 
 * command Exit.
 * 
 * @author Dezhi Ren
 *
 */

public class OutputForExit implements Output {

  /**
   * The function would print nothing.
   * 
   * @param content the content should be an empty string
   */
  public void giveOutput(String content) {
    if (content.equals(""))
      return;
    else
      System.out.println(content);

  }

}
